\begin_layout Plain Layout
\begin_inset Formula 
\begin{eqnarray*}
\left\Vert \omega\times Rb\right\Vert ^{2} & = & \omega^{2}\left(Rb\right)^{2}-\left(\omega^{T}Rb\right)^{2}\\
 & = & \omega^{T}\omega b^{T}R^{T}Rb-\omega^{T}Rb\omega^{T}Rb\\
 & = & \omega^{T}\omega b^{T}b-\omega^{T}Rbb^{T}R^{T}\omega\\
 & = & \omega^{T}RR^{T}\omega b^{T}b-\omega^{T}Rbb^{T}R^{T}\omega\\
 & = & \omega^{T}R\left[Ib^{T}b-bb^{T}\right]R^{T}\omega\\
 & = & \omega_{s}^{T}\left[Ib^{T}b-bb^{T}\right]\omega_{s}
\end{eqnarray*}

\end_inset


