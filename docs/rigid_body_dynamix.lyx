\left\Vert \dot{r}\right\Vert ^{2} & = & \left\Vert \dot{d}\right\Vert ^{2}+\omega_{s}^{T}\left[Ib^{T}b-bb^{T}\right]\omega+2\dot{d}^{T}\omega\times Rb
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
