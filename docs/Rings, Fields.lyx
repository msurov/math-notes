\begin_layout Plain Layout
\begin_inset Formula $\left(x^{5}-6x+3\right)/\left(x-x_{1}\right)=x^{4}+x^{3}x_{1}+x^{2}x_{1}^{2}+xx_{1}^{3}+\left(x_{1}^{4}-6\right)$
\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula $\left(x^{5}-6x+3\right)-x^{4}\left(x-x_{1}\right)=x^{5}-6x+3-x^{5}+x^{4}x_{1}=-6x+3+x^{4}x_{1}$
\end_inset


